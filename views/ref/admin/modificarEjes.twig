{% extends "ref/side-master.twig" %} {# ----------------------------------------------------------- #} {% block titulo %}Exportar{%
endblock %} {% block body %}
<div class="container-fluid" style=" ">
  <div class="row">
    <div class="col-xs-12 col-md-11 col-lg-8">
      <h2>Modificar Ejes</h2>
      <p class="text-muted">Seleccione el eje a modificar</p>
      <hr>
      <table class="table table-striped table-hover ">
        <thead>
          <tr>
            <th colspan="2">Ejes</th>
          </tr>
        </thead>
        <tbody>
          {% for i in 1..8 %}
          <tr>
            <td>Eje {{ i }}</td>
            <td class="text-right"><a href="{{ urlFor('shwModifDerecho', {'idDer': i}) }}" class="btn btn-link btn-xs">Modificar<i class="fa fa-pencil fa-fw"></i></a></td>
          </tr>
          {% endfor %}
        </tbody>
      </table>
    </div>
  </div>
</div>
{% endblock body %} {# -------------------------------------------- #} {% block scripts %} {% endblock scripts %}