	<link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ baseUrl() }}/assets-ref/img/favicon/57.png">
	<!-- For iPad -->
	<link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ baseUrl() }}/assets-ref/img/favicon/72.png">
	<!-- For iPad Retina display -->
	<link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ baseUrl() }}/assets-ref/img/favicon/144.png">
	<meta property="og:title" content="Inicio" />
	<meta property="og:type" content="article" />
	<meta property="og:url" content="http://basesparalareforma.digital/" />
	<meta property="og:site_name" content="Bases para la Reforma" />
	<meta property="og:description" content="Nuestra Constitución 
